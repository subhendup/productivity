               <padding>
                  <Insets top="3.0" />
               </padding>
            </AnchorPane>
         </center>
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
         <top>
            <HBox alignment="CENTER" disable="true" maxHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" BorderPane.alignment="CENTER">
               <children>
                  <Label styleClass="applicationSettingsLabel" text="" />
               </children>
            </HBox>
         </top>
      </BorderPane>
   </children>
</fx:root>
